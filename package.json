{
  "name": "links-v2",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "eslint \"src/**/*.ts\" \"libs/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" \"libs/**/*.ts\" --fix",
    "lint:html": "eslint \"src/**/*.html\" \"libs/**/*.html\"",
    "format": "prettier --write \"src/**/*.{ts,html,css,scss,json}\" \"libs/**/*.{ts,html,css,scss,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,html,css,scss,json}\" \"libs/**/*.{ts,html,css,scss,json}\"",
    "type-check": "tsc --noEmit",
    "check": "npm run format:check && npm run lint && npm run type-check",
    "fix": "npm run format && npm run lint:fix",
    "api:generate": "npm run api:generate:auth && npm run api:generate:links && npm run api:postgen",
    "api:generate:auth": "ng-openapi-gen -c src/api/ng-openapi-gen-auth.json",
    "api:generate:links": "ng-openapi-gen -c src/api/ng-openapi-gen-links.json",
    "api:postgen": "node scripts/api-postgen.mjs",
    "storybook": "ng run links-v2:storybook",
    "build-storybook": "ng run links-v2:build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.1.3",
    "@angular/cdk": "~21.1.3",
    "@angular/common": "^21.1.3",
    "@angular/compiler": "^21.1.3",
    "@angular/core": "^21.1.3",
    "@angular/forms": "^21.1.3",
    "@angular/material": "~21.1.3",
    "@angular/platform-browser": "^21.1.3",
    "@angular/platform-browser-dynamic": "^21.1.3",
    "@angular/router": "^21.1.3",
    "@anthropic-ai/claude-code": "^2.1.37",
    "@fontsource-variable/mona-sans": "^5.2.8",
    "@fontsource/material-icons": "^5.2.7",
    "@jsverse/transloco": "^8.2.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.2101.3",
    "@angular-devkit/build-angular": "^21.1.3",
    "@angular-devkit/core": "^21.1.3",
    "@angular-eslint/builder": "^21.2.0",
    "@angular-eslint/eslint-plugin": "^21.2.0",
    "@angular-eslint/eslint-plugin-template": "^21.2.0",
    "@angular-eslint/schematics": "^21.2.0",
    "@angular-eslint/template-parser": "^21.2.0",
    "@angular/cli": "^21.1.3",
    "@angular/compiler-cli": "^21.1.3",
    "@angular/material": "^21.1.3",
    "@compodoc/compodoc": "^1.2.1",
    "@eslint/js": "^8.57.1",
    "@storybook/addon-a11y": "^10.2.8",
    "@storybook/addon-docs": "^10.2.8",
    "@storybook/addon-onboarding": "^10.2.8",
    "@storybook/angular": "^10.2.8",
    "@types/jasmine": "~5.1.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-storybook": "^10.2.8",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-openapi-gen": "^1.0.5",
    "prettier": "^3.8.1",
    "storybook": "^10.2.8",
    "typescript": "~5.9.3",
    "typescript-eslint": "^7.2.0"
  }
}
